{
    "manifest_version": 3,
    "name": "Website Lock",
    "version": "1.0",
    "description": "Blocks access to websites until a password is entered.",
    "permissions": [
      "storage",
      "declarativeNetRequest",
      "tabs"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  